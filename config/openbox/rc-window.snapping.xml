<?xml version="1.0" encoding="UTF-8"?>
<!-- Do not edit this file, it will be overwritten on install.
        Copy the file to $HOME/.config/openbox/ instead. -->
<openbox_config xmlns="http://openbox.org/3.4/rc" xmlns:xi="http://www.w3.org/2001/XInclude">
<!-- Keybinding for window placing -->
    <keybind key="W-KP_1">        # BottomLeftScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>0</x><y>-0</y><height>49%</height><width>49%</width></action>
    </keybind>
    <keybind key="W-KP_2">        # HalfLowerScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>0</x><y>-0</y><width>100%</width><height>50%</height></action>
    </keybind>
    <keybind key="W-KP_3">        # BottomRightScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>-0</x><y>-0</y><height>49%</height><width>49%</width></action>
    </keybind>
    <keybind key="W-KP_4">        # HalfLeftScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>0</x><y>0</y><height>97%</height><width>50%</width></action>
    </keybind>
    <keybind key="W-KP_5">        # CenterScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>20%</x><y>20%</y><height>60%</height><width>60%</width></action>
    </keybind>
    <keybind key="W-KP_6">       # HalfRightScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>-0</x><y>0</y><height>97%</height><width>50%</width></action>
    </keybind>
    <keybind key="W-KP_7">        # TopLeftScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>0</x><y>0</y><height>49%</height><width>49%</width></action>
    </keybind>
    <keybind key="W-KP_8">          # HalfUpperScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>0</x><y>0</y><width>100%</width><height>50%</height></action>
    </keybind>
    <keybind key="W-KP_9">        # TopRightScreen
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo"><x>-0</x><y>0</y><height>49%</height><width>49%</width></action>
    </keybind>
</openbox_config>
